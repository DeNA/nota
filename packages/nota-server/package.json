{
  "name": "nota-server",
  "version": "2.4.1",
  "private": true,
  "main": "src/bin/www",
  "repository": {
    "type": "git",
    "url": "https://github.com/dena/nota"
  },
  "author": "Jonatan Alama <jonatan.alama@dena.com>",
  "scripts": {
    "app": "src/bin/www",
    "start": "cd src && ../node_modules/.bin/pm2-dev ecosystem.json --env dev",
    "start:web": "cd src && pm2-runtime ecosystem.json --only nota",
    "start:worker": "cd src && pm2-runtime ecosystem.json --only nota-worker",
    "test": "NODE_ENV=test node_modules/.bin/jest --detectOpenHandles --runInBand --forceExit",
    "test:staged": "NODE_ENV=test node_modules/.bin/jest --findRelatedTests --detectOpenHandles --runInBand --forceExit",
    "resetdb": "sequelize db:drop; sequelize db:create && sequelize db:migrate",
    "seed": "sequelize db:seed:all"
  },
  "dependencies": {
    "archiver": "^5.3.0",
    "aws-sdk": "^2.1005.0",
    "axios": "^0.22.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "1.19.0",
    "bullmq": "^1.49.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cron-parser": "^4.0.0",
    "debug": "^4.3.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-winston": "^4.2.0",
    "fs-extra": "^10.0.0",
    "helmet": "^4.6.0",
    "ioredis": "^4.27.11",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "mysql2": "^2.3.0",
    "node-decipher-openssl": "^1.0.4",
    "passport": "^0.5.0",
    "passport-cookie": "^1.0.9",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "passport-saml": "^3.2.0",
    "redlock": "^4.2.0",
    "sequelize": "^6.7.0",
    "serve-favicon": "^2.5.0",
    "sharp": "^0.29.1",
    "stream-buffers": "^3.0.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/redlock": "^4.0.2",
    "aws-sdk-mock": "^5.4.0",
    "child-process-promise": "^2.2.1",
    "concurrently": "^6.3.0",
    "jest": "^27.2.5",
    "mock-fs": "^5.1.1",
    "pm2": "^5.1.2",
    "sequelize-cli": "^6.2.0",
    "sqlite3": "^5.0.2",
    "supertest": "^6.1.6"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/client/"
    ],
    "collectCoverageFrom": [
      "src/models/*.js",
      "src/routes/*.js",
      "src/lib/**/*.js",
      "src/services/*.js",
      "!<rootDir>/node_modules/"
    ],
    "collectCoverage": true,
    "transform": {},
    "maxConcurrency": 1,
    "maxWorkers": 1,
    "setupFilesAfterEnv": [
      "./testSetup.js"
    ],
    "globalSetup": "./testGlobalSetup.js",
    "clearMocks": true,
    "roots": [
      "./src"
    ]
  }
}
