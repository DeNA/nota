{
  "name": "nota-server",
  "version": "2.7.0",
  "private": true,
  "main": "src/bin/www",
  "repository": {
    "type": "git",
    "url": "https://github.com/dena/nota"
  },
  "author": "Jonatan Alama <jonatan.alama@dena.com>",
  "scripts": {
    "app": "src/bin/www",
    "start": "cd src && ../node_modules/.bin/pm2-dev ecosystem.json --env dev",
    "start:web": "cd src && pm2-runtime ecosystem.json --only nota",
    "start:worker": "cd src && pm2-runtime ecosystem.json --only nota-worker",
    "test": "NODE_ENV=test node_modules/.bin/jest --detectOpenHandles --runInBand --forceExit",
    "test:staged": "NODE_ENV=test node_modules/.bin/jest --findRelatedTests --detectOpenHandles --runInBand --forceExit",
    "resetdb": "sequelize db:drop; sequelize db:create && sequelize db:migrate",
    "seed": "sequelize db:seed:all"
  },
  "dependencies": {
    "archiver": "3.0.0",
    "aws-sdk": "2.1009.0",
    "axios": "^0.21.1",
    "bcryptjs": "2.4.3",
    "body-parser": "1.18.3",
    "bullmq": "^1.42.1",
    "compression": "1.7.4",
    "cookie-parser": "1.4.4",
    "cron-parser": "^2.13.0",
    "csv-stringify": "4.3.1",
    "debug": "4.1.1",
    "dotenv": "^8.0.0",
    "express": "4.16.4",
    "express-winston": "^4.0.1",
    "fs-extra": "7.0.1",
    "helmet": "^3.21.1",
    "ioredis": "^4.14.0",
    "moment": "2.24.0",
    "moment-timezone": "^0.5.26",
    "mysql2": "1.6.5",
    "node-decipher-openssl": "1.0.4",
    "passport": "^0.4.0",
    "passport-cookie": "^1.0.6",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "passport-saml": "^3.2.1",
    "redlock": "^4.2.0",
    "sequelize": "^5.22.5",
    "serve-favicon": "2.5.0",
    "sharp": "0.29.0",
    "stream-buffers": "3.0.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/redlock": "^4.0.2",
    "aws-sdk-mock": "^5.4.0",
    "child-process-promise": "^2.2.1",
    "concurrently": "^5.3.0",
    "jest": "^27.0.6",
    "mock-fs": "^5.1.1",
    "pm2": "^5.1.1",
    "sequelize-cli": "^5.5.0",
    "sqlite3": "^4.0.9",
    "supertest": "^4.0.2",
    "swagger-jsdoc": "^3.2.9"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/client/"
    ],
    "collectCoverageFrom": [
      "src/models/*.js",
      "src/routes/*.js",
      "src/lib/**/*.js",
      "src/services/*.js",
      "!<rootDir>/node_modules/"
    ],
    "collectCoverage": true,
    "transform": {},
    "maxConcurrency": 1,
    "maxWorkers": 1,
    "setupFilesAfterEnv": [
      "./testSetup.js"
    ],
    "globalSetup": "./testGlobalSetup.js",
    "clearMocks": true,
    "roots": [
      "./src"
    ]
  }
}
