{
  "parser": "jsonv2",
  "mediaType": "VIDEO",
  "mediaExtensions": ["mp4"],
  "mediaOptions": {
    "predictionFileSuffix": "prediction",
    "predictions": ["prediction_1", "prediction_2"]
  },
  "options": {
    "showReview": true
  },
  "annotations": [
    {
      "name": "frame",
      "label": "Frame",
      "type": "MEDIA_LABELS",
      "labels": [
        {
          "name": "frame",
          "label": "Frame",
          "type": "TEXT_INPUT",
          "options": {
            "required": true,
            "regExp": "^[0-9]*$",
            "description": "Insert the frame number"
          }
        }
      ]
    }
  ]
}
